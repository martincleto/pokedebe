{
  "config": {
    "protocol": "http",
    "hostName": "pokeapi.co",
    "versionPath": "/api/v2/"
  },
  "pathMap": {
    "/": {
      "normalizePattern": "results",
      "wrapperMethod": "getPokemonsList"
    },
    "/detail/:name": {
      "normalizePattern": "default",
      "wrapperMethod": "getPokemonByName"
    }
  }
}
