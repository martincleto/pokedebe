{
  "moduleFileExtensions": [
    "js",
    "json",
    "jsx"
  ],
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
    "\\.(css|scss)$": "identity-obj-proxy",
    "Components/Detail": "<rootDir>/src/components/Detail.jsx",
    "Components/Footer": "<rootDir>/src/components/Footer.jsx",
    "Components/Header": "<rootDir>/src/components/Header.jsx",
    "Components/Search": "<rootDir>/src/components/Search.jsx",
    "Containers/App": "<rootDir>/src/containers/App.jsx",
    "Core/app": "<rootDir>/src/components/Search.jsx",
    "Core/config/api": "<rootDir>/src/core/config/api.json"
  },
  "setupFiles": ["<rootDir>/test/__mocks__/browserMocks.js"],
  "transform": {
    "^.+\\.(jsx?)$": "<rootDir>/node_modules/babel-jest/build",
    "^.+\\.(css|scss)$": "<rootDir>/scripts/jest-script-preprocessor.js"
  }
}
