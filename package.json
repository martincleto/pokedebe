{
  "name": "pokedebe",
  "version": "1.0.0",
  "author": "Santiago Mart√≠n-Cleto <martincleto@gmail.com>",
  "main": "index.html",
  "repository": "https://github.com/martincleto/pokedebe.git",
  "license": "GPL-3.0",
  "keywords": [
    "es6",
    "react",
    "pokemon",
    "pokeapi",
    "webpack"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js -p --progress --colors",
    "lint:js": "eslint --cache -c config/.eslintrc.json --cache-location cache/.eslintcache \"src/**/*.{js,jsx}\"; exit 0",
    "lint:sass": "sass-lint -c config/.sass-lint.yml -v -q",
    "lint": "yarn run lint:sass && npm run lint:js",
    "prebuild": "yarn run clean",
    "prestart": "yarn run clean",
    "pretest": "yarn run clean & yarn run lint",
    "start": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js -p && node server/server.js",
    "dev": "webpack-dev-server",
    "test": "jest --config config/jest.config.json -u --no-cache"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "mime-types": "^2.1.14",
    "react": "^15.4.2",
    "react-autosuggest": "^9.0.0",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "transform-runtime": "^0.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "eslint": "^3.16.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "2.0.0-rc.1",
    "fetch-mock": "^5.9.4",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.0",
    "normalize.css": "^5.0.0",
    "pokedex-promise-v2": "^3.0.3",
    "postcss-loader": "^1.3.1",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^3.0.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  }
}
