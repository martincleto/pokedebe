{
  "name": "pokedebe",
  "version": "1.0.0",
  "author": "Santiago Mart√≠n-Cleto <martincleto@gmail.com>",
  "main": "index.html",
  "repository": "https://github.com/martincleto/pokedebe.git",
  "license": "GPL-3.0",
  "keywords": [
    "es6",
    "react",
    "pokemon",
    "pokeapi",
    "webpack"
  ],
  "scripts": {
    "clean": "rm -rf public/dist",
    "build": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js -p --progress --colors",
    "lint:js": "eslint --cache -c config/.eslintrc.json --cache-location cache/.eslintcache src/**/*.js; exit 0",
    "lint:sass": "sass-lint -c config/.sass-lint.yml -v -q",
    "lint": "yarn run lint:sass && npm run lint:js",
    "prebuild": "yarn run clean",
    "prestart": "yarn run clean",
    "start": "webpack-dev-server",
    "test": "yarn run lint && karma start config/karma.conf.js"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "eslint": "^3.16.0",
    "extract-text-webpack-plugin": "2.0.0-rc.1",
    "html-webpack-plugin": "^2.28.0",
    "jasmine": "^2.5.3",
    "karma": "^1.5.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^2.0.2",
    "node-sass": "^4.5.0",
    "normalize.css": "^5.0.0",
    "postcss-loader": "^1.3.1",
    "react-addons-test-utils": "^15.4.2",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^3.0.0"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      [
        "latest",
        {
          "es2015": {
            "modules": false
          }
        }
      ],
      "react"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  }
}
